{
  "name": "lunr/ticks.monolog",
  "description": "Lunr.Ticks.Monolog is a library for sending monolog logs to the analytics store.",
  "keywords": [ "logging" ],
  "homepage": "https://github.com/lunr-php/lunr.ticks.monolog",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "lunr/ticks": "~0.12.0",
    "monolog/monolog": "~3.9",
    "psr/log": "~3.0"
  },
  "require-dev": {
    "phpunit/phpunit": ">=10.5 <11.0",
    "lunr/halo": "~0.11.0",
    "ext-xdebug": "~3.1"
  },
  "config": {
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {"Lunr\\": "src/Lunr/"}
  }
}
